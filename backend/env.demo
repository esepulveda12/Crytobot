# Configuración para cuenta demo de Binance
NODE_ENV=development
PORT=3000
CORS_ORIGIN=http://localhost:3000

# Binance Testnet (para pruebas)
BINANCE_BASE_URL=https://testnet.binance.vision
BINANCE_WS_URL=wss://testnet.binance.vision/ws

# Configuración de seguridad
JWT_SECRET=demo_jwt_secret_123456789012345678901234567890
ENCRYPTION_KEY=demo_encryption_key_32_chars_123456789012345678901234567890

# Configuración de logs
LOG_LEVEL=debug
LOG_FILE=logs/cryptobot-demo.log

# Configuración de rate limiting (más permisivo para demo)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=1000
